{
  "name": "web3goals-subgraph",
  "version": "0.2",
  "license": "UNLICENSED",
  "scripts": {
    "codegen-mumbai": "graph codegen subgraph-mumbai.yaml",
    "codegen-mumbai-production": "graph codegen subgraph-mumbai-production.yaml",
    "build": "graph build",
    "deploy-mumbai": "graph deploy --node https://api.thegraph.com/deploy/ kiv1n/web3-goals-dev subgraph-mumbai.yaml",
    "deploy-mumbai-production": "graph deploy --node https://api.thegraph.com/deploy/ kiv1n/web3-goals-dev subgraph-mumbai-production.yaml",
    "deploy-hyperspace": "graph deploy --node http://45.76.90.200:8020 --ipfs http://45.76.90.200:5001 kiv1n/web3-goals-dev subgraph-hyperspace.yaml",
    "create-local": "graph create --node http://localhost:8020/ kiv1n/web3-goals-dev",
    "remove-local": "graph remove --node http://localhost:8020/ kiv1n/web3-goals-dev",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 kiv1n/web3-goals-dev"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.30.2",
    "@graphprotocol/graph-ts": "0.27.0"
  }
}
